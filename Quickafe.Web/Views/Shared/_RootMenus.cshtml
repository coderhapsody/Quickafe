@model IEnumerable<Quickafe.Providers.ViewModels.RoleAccess.ListMenuViewModel>

@foreach (var menu in Model)
{
    <li class="dropdown">
        <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-expanded="false">@Html.Raw(!String.IsNullOrEmpty(menu.FAIcon) ? @"<i class='fa " + menu.FAIcon + @"'></i> " : "") @menu.Name <span class="caret"></span></a>
        @Html.Action("GetMenus", "Home", new { parentMenuId = menu.id ,area = String.Empty})
    </li>

}
