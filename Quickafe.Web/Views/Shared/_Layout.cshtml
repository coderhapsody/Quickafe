@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <title>@Strings.ApplicationTitle</title>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
    @Html.Partial("_References")
    @RenderSection("head", false)
    <script>
        $(function () {
            kendo.culture("id-ID");
            $.ajaxSetup({ cache: false });
        });
    </script>
</head>
<body>
    <!-- Fixed navbar -->
    <nav class="navbar navbar-inverse navbar-fixed-top" role="navigation">
        <div class="container-fluid">
            @Html.Partial("_LeftNavBar")
            <div id="navbar" class="navbar-collapse collapse">
                @Html.Partial("_MainNav")
                @Html.Partial("_RightNavBar")
            </div><!--/.nav-collapse -->
        </div>
    </nav>

    <div class="container">
        @RenderBody()
    </div> <!-- /container -->

    @Html.Partial("_PredefinedHiddenValues")

    <script src="~/Scripts/modules/app.js"></script>
    <input type="hidden" id="reportUrl" name="reportUrl" value="@Url.Content("~/Reports/ReportPreview.aspx")"  />
    @RenderSection("scripts", false)
</body>
</html>
